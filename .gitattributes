# Set the default behavior, in case people don't have core.autocrlf set.
* text=auto

# Text files: force them to be committed as native line ending (LF/Unix)
*.asax text
*.ashx text
*.aspx text
*.bat text eol=lf
*.config text
*.cs text eol=lf
*.cshtml text eol=lf
*.csproj text
*.htm text
*.html text
*.js text eol=lf
*.jsb text
*.json text eol=lf
*.md text
*.php text eol=lf
*.ps1 text
*.py text eol=lf
*.sln text
*.sql text eol=lf
*.d.ts text eol=lf
*.target text
*.txt text eol=lf
*.xml text

# forces .sh files to be checked out as LF line endings
*.sh text eol=lf

# These are expected to be always treated as binary files.
*.bmp binary
*.dat binary
*.dll binary
*.exe binary
*.ico binary
*.jar binary
*.jpg binary
*.png binary
